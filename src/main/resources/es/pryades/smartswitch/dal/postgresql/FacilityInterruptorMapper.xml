<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="es.pryades.smartswitch.dal.ibatis.FacilityInterruptorMapper">

	<insert id="addRow" parameterType="es.pryades.smartswitch.dto.FacilityInterruptor">

		insert into facility_interruptors
		(
			ref_facility,
			ref_interruptor
		)
		values
		(
			#{ref_facility},
			#{ref_interruptor}
		)
	</insert>

</mapper>
